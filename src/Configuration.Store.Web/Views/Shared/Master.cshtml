<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>

    <!--Import materialize.css-->
    <link type="text/css" rel="stylesheet" href="~/Styles/materialize/css/materialize.css" media="screen,projection" />
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">

    <!--Let browser know website is optimized for mobile-->
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />

    <script type="text/javascript" src="~/Scripts/lib/jquery/jquery.js"></script>
    <script type="text/javascript" src="~/Scripts/lib/materialize/materialize.js"></script>

    <script type="text/javascript" src="~/Scripts/lib/react/react.development.js"></script>
    <script type="text/javascript" src="~/Scripts/lib/react/react-dom.development.js"></script>

    <script type="text/javascript" src="~/Scripts/lib/require/require.js"></script>

    <script type="text/javascript">
        requirejs(['Scripts/lib/promise/es6-promise.auto'], function (polyfill) {});
    </script>

    <script type="text/javascript" src="~/Scripts/lib/fetch/fetch.js"></script>
    
    @RenderSection("Scripts")
</head>
<body class="grey lighten-4">
    <header>
        <div class="navbar-fixed">
            <nav>
                <div class="nav-wrapper light-blue accent-3">
                    <a href="~/" class="brand-logo center">Configuration Store</a>
                </div>
            </nav>
        </div>
    </header>
    <div id="root" class="container grey-text text-darken-3"></div>
    @RenderBody()
</body>
</html>