<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>

    <!--Import materialize.css-->
    <link type="text/css" rel="stylesheet" href="Styles/materialize/css/materialize.css" media="screen,projection" />

    <!--Let browser know website is optimized for mobile-->
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />

    <!--Import jQuery before materialize.js-->
    <script type="text/javascript" src="Scripts/lib/jquery/jquery.js"></script>
    <script type="text/javascript" src="Scripts/lib/materialize/materialize.js"></script>

    <script type="text/javascript" src="Scripts/lib/react/react.js"></script>
    <script type="text/javascript" src="Scripts/lib/react/react-dom.js"></script>

    <script type="text/javascript" src="Scripts/lib/require/require.js"></script>
    
    @RenderSection("Scripts")
</head>
<body>
    <div id="root"></div>
    @RenderBody()
</body>
</html>